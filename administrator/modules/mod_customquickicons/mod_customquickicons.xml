<?xml version="1.0" encoding="utf-8"?>
<install type="module" version="1.5.0" client="administrator">
	<name>CustomQuickIcons</name>
	<author>mic</author>
	<creationDate>2008.10.01</creationDate>
	<copyright>(C) 2006/7/8 joomX.com All rights reserved.</copyright>
	<license>http://www.gnu.org/copyleft/gpl.html GNU/GPL</license>
	<authorEmail>info@joomx.com</authorEmail>
	<authorUrl>www.joomx.com</authorUrl>
	<version>2.1.0</version>
	<description>Module description</description>

	<languages folder="admin/language">
		<language tag="de-DE">de-DE/de-DE.mod_customquickicons.ini</language>
		<language tag="en-GB">en-GB/en-GB.mod_customquickicons.ini</language>
	</languages>

	<files>
    	<filename module="mod_customquickicons">mod_customquickicons.php</filename>
	</files>
	<params>
		<param type="spacer" default="(c) 2008 www.joomaax.com" />
		<param type="radio" name="cache" default="1" label="Cache" description="Cache description">
		    <option value="1">Use global</option>
		    <option value="0">No</option>
    	</param>
		<param name="css" type="radio" default="1" label="CSS Class" description="CSS Description">
			<option value="1">Yes</option>
		    <option value="0">Use global</option>
		</param>
	</params>
	<params group="advanced">
		<param type="radio"
			name="user_access"
			label="User access"
			description="Show user icons"
			default="0">
			<option value="1">Yes</option>
		    <option value="0">No</option>
		</param>
	</params>
</install>